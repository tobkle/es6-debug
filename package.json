{
    "name": "debug-es6",
    "version": "1.0.0",
    "private": true,
    "main": ".compiled",
    "author": "Tobias Klemmer <tobias@klemmer.info>",
    "license": "MIT",
    "scripts": {
        "compile": "babel api/src --out-dir api/dist --source-maps --watch",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "vue": "^2.5.17"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@vue/cli-plugin-babel": "^3.0.4",
        "@vue/cli-plugin-eslint": "^3.0.4",
        "@vue/cli-service": "^3.0.4",
        "babel-loader": "^8.0.4",
        "eslint": "^4.16.0",
        "vue-template-compiler": "^2.5.17"
    },
    "babel": {
        "presets": [
            "@vue/app",
            "@babel/env"
        ]
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 7,
            "sourceType": "module",
            "parser": "babel-eslint"
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
