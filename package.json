{
    "name": "debug-es6",
    "version": "1.0.0",
    "main": ".compiled",
    "author": "Tobias Klemmer <tobias@klemmer.info>",
    "license": "MIT",
    "scripts": {
        "compile": "babel src --out-dir .compiled --source-maps --watch"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.5.0",
        "eslint": "^4.16.0"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 7,
            "sourceType": "module"
        },
        "env": {
            "node": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    }
}